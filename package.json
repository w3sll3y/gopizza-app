{
  "name": "gopizza",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "codepush:ios": "appcenter codepush release-react -a Quarks/casa-video-ios -d Production -m",
    "codepush:android": "appcenter codepush release-react -a Quarks/casa-video-android -d Production -m",
    "build:codepush": "yarn codepush:ios && yarn codepush:android",
    "clean": "rm -rf node_modules yarn.lock && cd ios && rm -rf Pods Podfile.lock && cd ../android/app && rm -rf build && cd .. && rm -rf .gradle && cd .. && yarn && cd ios && pod install",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "pod": "npx pod-install",
    "ios:beta": "yarn test && bundle exec fastlane ios beta",
    "ios:release": "yarn test && bundle exec fastlane ios release",
    "android:beta": "yarn test && bundle exec fastlane android beta",
    "android:release": "yarn test && bundle exec fastlane android release",
    "android:apk": "yarn test && bundle exec fastlane android apk"
  },
  "dependencies": {
    "@expo-google-fonts/dm-sans": "^0.2.2",
    "@expo-google-fonts/dm-serif-display": "^0.2.2",
    "@react-native-firebase/app": "^15.1.1",
    "@react-native-firebase/auth": "^15.1.1",
    "@react-native-firebase/firestore": "^15.1.1",
    "@react-native-firebase/storage": "^15.1.1",
    "expo": "~45.0.0",
    "expo-app-loading": "~2.0.0",
    "expo-linear-gradient": "~11.3.0",
    "expo-splash-screen": "~0.15.1",
    "expo-status-bar": "~1.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.2",
    "react-native-gesture-handler": "~2.2.1",
    "react-native-iphone-x-helper": "^1.3.1",
    "react-native-web": "0.17.7",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.67.6",
    "@types/styled-components-react-native": "^5.1.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "typescript": "~4.3.5"
  },
  "private": true
}
